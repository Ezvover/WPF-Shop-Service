<Window x:Class="laba4.AddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:laba4"
        mc:Ignorable="d"
        Title="AddWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="DescTextBox" HorizontalAlignment="Left" Margin="72,136,0,0" TextWrapping="Wrap" Text="Desc" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="CategoryTextBox" HorizontalAlignment="Left" Margin="72,171,0,0" TextWrapping="Wrap" Text="Category" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="RateTextBox" HorizontalAlignment="Left" Margin="72,0,0,0" TextWrapping="Wrap" Text="Rate" VerticalAlignment="Center" Width="120"/>
        <TextBox x:Name="PriceTextBox" HorizontalAlignment="Left" Margin="72,244,0,0" TextWrapping="Wrap" Text="Price" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="AmountTextBox" HorizontalAlignment="Left" Margin="72,279,0,0" TextWrapping="Wrap" Text="Amount" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="OtherTextBox" HorizontalAlignment="Left" Margin="72,311,0,0" TextWrapping="Wrap" Text="Other" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="AddButton" Content="Add" HorizontalAlignment="Left" Margin="250,0,0,0" VerticalAlignment="Center" RenderTransformOrigin="0.721,0.595" Height="239" Width="150" Click="AddButton_Click"/>
        <TextBox x:Name="NameTextBox" HorizontalAlignment="Left" Margin="72,98,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
